{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Audience": {
    "Secret": "ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890",
    "Issuer": "gsw",
    "Audience": "everone"
  },
  //实现api聚合
  "ReverseProxy": {
    "Routes": {
      //业务服务admin    
      "adminservice": {
        "ClusterId": "admin_cluster",
        //"AuthorizationPolicy": "Permission",
        "Match": {
          "Path": "/admin/{**catch-all}"
        }
      },
      //业务服务auth
      "authservice": {
        "ClusterId": "auth_cluster",
        //"AuthorizationPolicy": "Permission",
        "Match": {

          "Path": "/auth/{**catch-all}"
        }
      },
      //业务服务equip
      "equipservice": {
        "ClusterId": "equip_cluster",
        "Match": {
          "Path": "/equip/{**catch-all}"
        }
      },
      //业务服务plan
      "planservice": {
        "ClusterId": "plan_cluster",
        "Match": {
          "Path": "/plan/{**catch-all}"
        }
      },
      //业务服务report
      "reportservice": {
        "ClusterId": "report_cluster",
        "Match": {
          "Path": "/report/{**catch-all}"
        }
      },
      //业务服务stroe
      "storeservice": {
        "ClusterId": "store_cluster",
        "Match": {
          "Path": "/store/{**catch-all}"
        }
      },
      //业务服务tracker
      "trackerservice": {
        "ClusterId": "tracker_cluster",
        "Match": {
          "Path": "/tracker/{**catch-all}"
        }
      },
      //业务服务web
      "webservice": {
        "ClusterId": "web_cluster",
        "Match": {
          "Path": "web/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "admin_cluster": {
        "HealthCheck": {
          "Active": {
            "Enabled": "true",
            "Interval": "00:00:10",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
            "Path": "/admin/health"
          }
        },
        "Metadata": {
          "ConsecutiveFailuresHealthPolicy.Threshold": "5"
        },
        "Destinations": {
          "admin_cluster/destination": {
            "Address": "http://localhost:9001/"
          }
        }
      },
      "auth_cluster": {
        "HealthCheck": {
          "Active": {
            "Enabled": "true",
            "Interval": "00:00:10",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
